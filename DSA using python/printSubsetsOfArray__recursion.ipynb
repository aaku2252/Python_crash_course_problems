    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "def print_subsets(arr):\n",
    "    n = len(arr)\n",
    "    for i in range(2**n):\n",
    "        subset = []\n",
    "        for j in range(n):\n",
    "            if i & (1<<j):\n",
    "                subset.append(arr[j])\n",
    "        print(*subset)\n",
    "\n",
    "print_subsets(arr)"
   ]
