    "def removeX(string):\n",
    "    if len(string)==0 or len(string)==1:\n",
    "        return ''\n",
    "    if string[0:2]=='pi' :        \n",
    "        return '3.14'+removeX(string[2:])\n",
    "    else:\n",
    "        return string[0]+removeX(string[1:])\n",
    "\n",
    "    \n",
    "    \n",
    "\n",
    "print(removeX('aspippidefxpipi'))"
   ]
  }
